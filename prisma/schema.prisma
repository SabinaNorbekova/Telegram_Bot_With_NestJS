generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  name      String?
  age       Int?
  phone     String?
  region    String?
  district  String?
  isActive  Boolean  @default(false)
  createdAt DateTime @default(now())
}